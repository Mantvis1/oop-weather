{% extends "base.twig" %}

{% block content %}
    <div class="row ">
        <div class="col-lg-12">
            <div class="wrapper wrapper-content">
                <div class="row">
                    <div class="col-lg-4"></div>
                    <div class="col-lg-3">
                        {% if weather.high != null %}
                            {{ include('new-weather-block.twig', {item: weather}) }}
                        {% else %}
                            {{ include('weather-block.twig', {item: weather}) }}
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
